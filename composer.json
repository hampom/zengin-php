{
    "name": "zengin-code/zengin-php",
    "description": "The PHP implementation of ZenginCode",
    "type": "library",
    "keywords": ["zengin", "bank", "japan", "banking", "financial"],
    "license": "MIT",
    "authors": [
        {
            "name": "anazawa yasuhiro",
            "email": "yasuhiro@humming-code.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "ZenginCode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZenginCode\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
